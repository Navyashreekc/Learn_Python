# Arithmetic operator
x=10
y=20

print(x+y)  #addition
print(x-y)  #subtraction
print(x*y)  #multiplication
print(x/y)  #division 
print(x//y) #floor division
print(x**y) #exponentiation
print(x%y)  #modulus

#additional operators

print(-x)    #negation
print(+y)    #positive
print(abs(-x)) #absolute value
print(round(3.7)) #rounding off
print(round(3.2)) #rounding off
print(pow(2,3))  #power function
print(sum([x,y])) #sum function
print(min(x,y))  #minimum value
print(max(x,y))  #maximum value
print(divmod(y,x)) #returns quotient and remainder as a tuple
print(int(3.9))   #type conversion to integer
print(float(x))   #type conversion to float
print(complex(x,y)) #type conversion to complex number
print(complex(2,3)) #complex number
print(complex(2))   #complex number with 0 imaginary part
print(bin(x))      #binary representation
print(oct(x))      #octal representation
print(hex(x))      #hexadecimal representation
print(format(x, 'b')) #binary format
print(format(x, 'o')) #octal format
print(format(x, 'x')) #hexadecimal format
print(chr(65))    #character from ASCII value
print(ord('A'))   #ASCII value from character
print(round(2.675, 2)) #rounding off with precision
print(round(2.685, 2)) #rounding off with precision
print(int('1010', 2)) #binary to integer conversion
print(int('12', 8))   #octal to integer conversion
print(int('A', 16))   #hexadecimal to integer conversion
print(float('3.14'))  #string to float conversion
##arithmetic operator with strings

a = "Navya \n" * 4

print(a)

## Comparision operators

x=2
y=8

print(x>y)
print(x<y)
print(x<=y)
print(x>=y)

## Logical Operators (and, or, not)
True and True
True and False
True or False
False or False
not True
not False
## Membership operators
1 in [1, 2, 3]
10 in [1, 2, 3]
10 not in [1, 2, 3]
"cat" in "my pet cat"
